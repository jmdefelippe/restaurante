<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.ItemComanda" table="itemComanda">
		
		<id column="idItemComanda" name="idItemComanda"><generator class="identity"/></id>
		
		<property column="cantidad" name="cantidad" type="int"/>
		
		<many-to-one name = "componenteMenu" class = "datos.ComponenteMenu" column = "idComponenteMenu" not-null = "true"/>
		<many-to-one name = "comanda" class = "datos.Comanda" column = "idComanda" not-null = "true"/>

	</class>
</hibernate-mapping>