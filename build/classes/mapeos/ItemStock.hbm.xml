<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.ItemStock" table="itemStock">
		
		<id column="idItemStock" name="idItemStock"><generator class="identity"/></id>
		
		<property column="cantidad" name="cantidad" type="int"/>
		
		<many-to-one name = "ingrediente" class = "datos.Ingrediente" column = "idIngrediente" not-null = "true"/>
		<many-to-one name = "stock" class = "datos.Stock" column = "idStock" not-null = "true"/>

	</class>
</hibernate-mapping>